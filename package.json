    "mustache": "^4.2.0",
    "mysql2": "^2.3.3",
    "node-dubbo": "git+http://gitlab.perfma-inc.com/fed/node/client-node-dubbo.git#8d79235a75446865f568f056e0273eedda0862b3",
    "node-t2sdk": "git+http://gitlab.perfma-inc.com/fed/node/client-node-t2sdk.git#0acbbad8d85bd86dd099144ff1343ac844d00a2a",
    "node-tzt": "git+http://gitlab.perfma-inc.com/fed/node/client-node-tzt.git#9ad92950b4e868c68e55550c4729dc50e5c2d1da",
    "oracledb": "git+http://gitlab.perfma-inc.com/fed/node/client-node-oracledb.git#3fe59ca136c79b5a5400dd3bf22963e86017b9aa",
    "papaparse": "^5.3.2",
    "pg": "^8.8.0",
    "pg-pool": "^3.5.2",
    "proper-lockfile": "^4.1.2",
    "socket.io": "^4.5.3",
    "tough-cookie": "^4.1.2",
    "uuid": "^9.0.0",
    "webdriverio": "^7.25.4",
    "xlsx": "git+http://gitlab.perfma-inc.com/fed/node/client-sheetjs.git#1870e541d4eb00d699d3a9b10f78a6e154ba3776",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@swc/core": "^1.3.11",
    "@types/amqplib": "^0.8.2",
    "@types/benchmark": "^2.1.2",
    "@types/chai": "^4.3.1",
    "@types/crypto-js": "^4.1.1",
    "@types/ioredis": "^5.0.0",
    "@types/js-to-java": "^2.7.1",
    "@types/lodash": "^4.14.186",
    "@types/mime": "^3.0.1",
    "@types/mocha": "^10.0.0",
    "@types/mssql": "^8.1.1",
    "@types/node": "^18.11.7",
    "@types/oracledb": "^5.2.3",
    "@types/papaparse": "^5.3.5",
    "@types/pg-pool": "^2.0.3",
    "@types/proper-lockfile": "^4.1.2",
    "@types/tough-cookie": "^4.0.2",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "benchmark": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "javascript-obfuscator": "^4.0.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
