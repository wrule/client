{
  "name": "x-engine",
  "version": "0.0.40",
  "description": "A high-performance test execution engine",
  "author": "William Chan",
  "preview": true,
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "bin": "dist/index.js",
  "pkg": {
    "scripts": [
      "dist/*.js",
      "node_modules/lodash/**/*.js",
      "node_modules/crypto-js/**/*.js",
      "node_modules/fast-xml-parser/src/parser.js",
      "node_modules/cheerio/lib/index.js",
      "node_modules/moment/moment.js",
      "node_modules/uuid/dist/index.js",
      "node_modules/chai/index.js",
      "node_modules/better-mock/dist/mock.node.js",
      "node_modules/mime/**/*.js",
      "node_modules/jsonpath-plus/dist/index-node-cjs.cjs",
      "node_modules/xlsx/xlsx.js",
      "node_modules/multi-crypto-js/index.js",
      "node_modules/yaml/dist/index.js",
      "node_modules/mustache/mustache.js"
    ],
    "assets": [
      "node_modules/oracledb/build/Release/oracledb.node",
      "node_modules/bufferutil/build/Release/bufferutil.node",
      "node_modules/utf-8-validate/build/Release/validation.node",
      "node_modules/node-t2sdk/build/Release/t2sdk.node"
    ]
  },
  "scripts": {
    "dev": "nodemon --inspect src/dev/index.ts",
    "dev-start": "nodemon --inspect src/index.ts",
    "dev-server": "nodemon --inspect src/dev/server.ts",
    "dev-debug": "nodemon --inspect src/dev/debug.ts",
    "dev-cli": "node --inspect --trace-warnings -r ts-node/register -r tsconfig-paths/register src/index.ts < ~/Documents/10000.json --output /dev/null",
    "dev-mock": "nodemon --inspect mock/dev/index.ts",
    "nodemon": "nodemon --inspect",
    "test": "nyc --reporter=text --reporter=html mocha --parallel",
    "lint": "eslint --fix --ext .js,.ts --cache --cache-location node_modules/.cache/.eslintcache src plugin",
    "benchmark": "nodemon src/dev/benchmark/index.ts",
    "release": "npm run build && rimraf bin && node -r ts-node/register pkg.ts",
    "build": "patch-package && rimraf dist && cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "patch": "patch-package",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "@mongosh/browser-runtime-electron": "^1.6.0",
