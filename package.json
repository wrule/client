    "rollup-plugin-typescript2": "^0.34.1",
    "socket.io-client": "^4.5.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "yargs": "^17.6.0"
  },
  "optionalDependencies": {
    "bufferutil": "websockets/bufferutil.git",
    "utf-8-validate": "websockets/utf-8-validate.git"
  }
}
