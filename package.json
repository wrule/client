    "decimal.js": "^10.4.2",
    "fast-xml-parser": "^4.0.11",
    "got": "11.8.5",
    "hpagent": "^1.0.0",
    "http2-wrapper": "^2.1.11",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.2.3",
    "js-to-java": "^2.7.0",
    "jsonpath-plus": "^7.2.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
